        "    "
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "YBmD2ZT0palp",
        "outputId": "5b836f20-26f3-41e4-e153-faa70a1f2e3c"
      },
      "execution_count": 62,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "5 * 10 is :50\n",
            "5 * 10 is :50\n",
