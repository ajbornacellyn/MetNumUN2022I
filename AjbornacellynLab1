      "cell_type": "code",
      "source": [
        "def mult_function(*args):\n",
        "    mult=1\n",
        "    for i in args:\n",
        "        mult*=i\n",
        "    return mult\n",
        "\n",
        "print(mult_function(1,2,3,4,5))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
