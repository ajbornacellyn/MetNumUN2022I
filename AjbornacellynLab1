      "source": [
        "def is_prime(Number):\n",
        "    x=Number-1\n",
        "    while x>1:\n",
        "        if Number % x==0:\n",
        "            return False\n",
        "        x-=1\n",
        "    return True\n",
        "\n",
        "is_prime(10)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "IAPUsvAytVFs",
        "outputId": "10395aea-1249-40ba-86a2-a78f552e2865"
      },
      "execution_count": 87,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "False"
            ]
          },
          "metadata": {},
          "execution_count": 87
        }
      ]
