      }
    },
    {
      "cell_type": "code",
      "source": [
        "dict_example[\"hamburgger\"] = 2555\n",
        "print(dict_example[\"hamburgger\"])"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
